<!ELEMENT AvailableFeatures (AvailableTemplateFeature* , Module* , AvailableFeature* )>

<!ELEMENT AvailableTemplateFeature (AdditionalXMLs+ , FeatureName*)>
<!ATTLIST AvailableTemplateFeature Name CDATA #IMPLIED>
<!ATTLIST AvailableTemplateFeature FeatureGUID CDATA #IMPLIED>
<!ATTLIST AvailableTemplateFeature ObjectCLSID CDATA #IMPLIED>
<!ATTLIST AvailableTemplateFeature FlexGridSchema CDATA #IMPLIED>


<!ELEMENT Module (AvailableFeature*)>
<!ATTLIST Module ModuleId CDATA #IMPLIED>
<!-- No need to mention Module Id as it is numeric , hence will have no problems with lang conversion -->

<!ELEMENT AvailableFeature (AdditionalXMLs* , SubFeature* , FeatureName* )>
<!ATTLIST AvailableFeature Name CDATA #IMPLIED>
<!ATTLIST AvailableFeature Order CDATA #IMPLIED>
<!ATTLIST AvailableFeature FeatureGUID CDATA #IMPLIED>
<!ATTLIST AvailableFeature ObjectCLSID CDATA #IMPLIED>
<!ATTLIST AvailableFeature FlexGridSchema CDATA #IMPLIED>

<!ELEMENT AdditionalXMLs (AdditionalXML*)>

<!ELEMENT AdditionalXML (#PCDATA)>
<!ATTLIST AdditionalXML ObjectName CDATA #IMPLIED> 
<!ATTLIST AdditionalXML XMLFileName CDATA #IMPLIED> 
<!ATTLIST AdditionalXML I18NRequired CDATA #IMPLIED> 

<!ELEMENT SubFeature (#PCDATA)>
<!ATTLIST SubFeature Name CDATA #IMPLIED> 
<!ATTLIST SubFeature Order CDATA #IMPLIED>
<!ATTLIST SubFeature FeatureGUID CDATA #IMPLIED> 
<!ATTLIST SubFeature ObjectCLSID CDATA #IMPLIED>
<!ATTLIST SubFeature FlexGridSchema CDATA #IMPLIED>

<!ELEMENT FeatureName (#PCDATA)>

