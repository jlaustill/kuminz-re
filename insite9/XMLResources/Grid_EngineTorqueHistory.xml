				<Column order = "5" datasource ="OBJ:objROW:getAttribute(PEAK_TORQUE_DURATION)" />
				<Object name="objSUBROWLIST" source="OBJ:objROW:childNodes" />
				<Node level = "1" loopStart="0" loopEnd="OBJ:objSUBROWLIST:length">
					<Object name="objSUBROW" source="OBJ:objSUBROWLIST:nextNode"/>
					<Column order = "1" datasource ="OBJ:objSUBROW:getAttribute(DESCRIPTION)" />
					<Column order = "2" datasource ="OBJ:objSUBROW:getAttribute(VALUE)" />
				</Node>
			</Node>
		</Node>
	</data>
	
