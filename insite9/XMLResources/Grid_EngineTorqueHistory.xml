	<data loopEnd="1">
		<Object name="objROWLIST" source="OBJ:DOM:selectNodes(ROW)"/>
		<Node level = "0" loopStart="OBJ:DOM_ENBLTKN:selectSingleNode(pPTPEnable):getAttribute(V)" loopEnd="1" styleid="s0">
      <!-- Feature Not Supported -->
      <Column order = "0" datasource ="RES:Res1:122" />
		</Node>
		<Node level = "0" loopStart="0" loopEnd="OBJ:DOM_ENBLTKN:selectSingleNode(pPTPEnable):getAttribute(V)" styleid="s1">
			<Node level = "0" loopStart="0" loopEnd="OBJ:objROWLIST:length" styleid="s1">
				<Object name="objROW" source="OBJ:objROWLIST:nextNode"/>
				<Column order = "0" datasource ="OBJ:objROW:getAttribute(ECM_TIME)" />
				<Column order = "1" datasource ="OBJ:objROW:getAttribute(HIGH_POWER_RATING)" />
				<Column order = "2" datasource ="OBJ:objROW:getAttribute(LOW_POWER_RATING)" />
				<Column order = "3" datasource ="OBJ:objROW:getAttribute(PEAK_HIGH_TORQUE)" />
